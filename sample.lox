class Cake {
    taste(adjective) {
      print "The " + this.flavour + " is tastes very " + adjective + ".";
    }
}

class Bakery {
  getSnack() {
      this.snack = Cake();
      this.snack.flavour = "Cheese cake";
      this.snack.taste("good");
  }

  callbackToTaste() {
    var bakery = Bakery();
    return bakery.getSnack;
  }
}


var bakery = Bakery();

var tasteWhenReady = bakery.callbackToTaste();
tasteWhenReady();

print this;