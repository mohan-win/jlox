class Cake {
  taste() {
    var adjective = "delicious";
    print "The " + this.flavour + " is very " + adjective;
  }
}

var cake = Cake();
cake.flavour = "Cheese cake";
cake.taste();

class Thing {
  getCallback() {
    fun localFunction() {
      print this;
    }

    return localFunction;
 }
}

var callback = Thing().getCallback();
callback();