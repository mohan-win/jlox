class Sweet {
  init(flavor) {
    this.flavor = flavor;
  }
  cook() {
    this.flavor = "cooked " + this.flavor;
  }
}

class Cake < Sweet {
  init(type, flavor) {
    this.type = type;
    super.init(flavor);
  }

  cook() {
    this.type = "cooked " + this.type;
    super.cook();
  }

  whoAmI() {
    print "This is a " + this.type + " of flavor " + this.flavor;
  }
}

var c = Cake("Cheese cake", "Strawberry");
c.whoAmI();