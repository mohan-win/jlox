class Doughnut {
  init(type) {
    this.type = type;
    inner(10);
  }
  cook() {
    print "Fry until golden brown";
    inner();
    print "Place in a nice box";
  }
}

class I < Doughnut {
  init(i) {
    print i;
  }
}

class BostonCream < I {
  cook() {
    print "Pipe full of custard and coat with chocolate";
    inner();
  }
}

class J < BostonCream {
  cook() {
    print "Enjoy!!";
  }
}

J("chocolate").cook();