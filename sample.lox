class Cake {
    init(flavour) {
      this.flavour = flavour;
      return;
    }
    taste(adjective) {
      print "The " + this.flavour + " tastes very " + adjective + ".";
    }
}

fun init() {
  return "This is some function called init";
}

var c = Cake("ok");
c.init = init;
print c.init();
c.taste("good");

